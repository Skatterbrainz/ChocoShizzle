<?xml version="1.0" encoding="ISO-8859-1"?>
<configuration version="1.0.5" comment="windows device configuration control file">
  <scriptsource url="https://raw.githubusercontent.com/Skatterbrainz/Chocolatey/master/Invoke-FudgePack-002.ps1" />
  <controls>
    <control enabled="true" type="logpath" value="c:\programdata\fudgepack\fudgepack.log" />
    <control enabled="false" type="logsize" value="5 MB" />
    <control enabled="true" type="logage" value="5 days" />
  </controls>
  <deployments>
    <deployment device="all" user="all" enabled="false" when="now">7zip,googlechrome</deployment>
    <deployment device="d004" user="Mike Hunt" enabled="true" when="now">office365proplus</deployment>
    <deployment device="d005" user="Mike Rotch" enabled="true" when="10/27/2017 10:00 PM">office365proplus</deployment>
    <deployment device="nsa18" user="Skatterbrainz" enabled="true" when="10/27/2017 10:00 PM">slack,notepadplusplus,git,vlc,putty,vlc,rdcman,office365proplus,microsoft-teams,skypeforbusiness,jing,teamviewer,visualstudiocode,vscode-powershell,vscode-icons,vscode-mssqdl,vscode-azurerm-tools,microsoftazurestorageexplorer,visualstudio2017community</deployment>
  </deployments>
  <removals>
    <removal device="all" user="all" enabled="false" when="now">firefox</removal>
    <removal device="d004" user="Mike Hunt" enabled="true" when="now" params="-yn --all-versions">notepadplusplus,git,7zip.install,vlc,putty,jing,teamviewer,vscode-powershell,vscode-icons,vscode-azurerm-tools,VisualStudioCode,firefox</removal>
    <removal device="d005" user="Mike Rotch" enabled="false" when="now"></removal>
    <removal device="nsa18" user="Skatterbrainz" enabled="false" when="10/27/2017 9:45 PM"></removal>
  </removals>
  <folders>
    <folder device="all" enabled="true" action="create" path="c:\programdata\fudgepack\test" />
    <folder device="all" enabled="true" action="empty" path="c:\windows\temp" filter="*.*" />
    <folder device="d004" enabled="false" action="empty" path="c:\windows\temp" filter="*.log" />
  </folders>
  <files>
    <file device="d004" enabled="false" action="download" source="" target="" />
    <file device="d005" enabled="false" action="rename" source="" target="" />
    <file device="d005" enabled="false" action="move" source="" target="" />
  </files>
  <registry>
    <reg device="all" enabled="true" path="HKLM:\software\fudgepack" value="version" data="1.0.5" type="string" />
  </registry>
  <services>
      <service device="all" enabled="true" name="RemoteRegistry" startup="automatic" action="start" />
  </services>
</configuration>
